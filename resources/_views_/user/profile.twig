{% extends 'layout.twig' %}

{% block title %}用户信息明细{% endblock %}

{% block content %}
  {% if users|length > 0 %}
    <ul>
    {% for user in users %}
      <li>{{ user.username }}</li>
    {% endfor %}
    </ul>
  {% else %}
  <p class="am-text-default am-text-center">No users have been found.</p>
  {% endif %}
{% endblock %}